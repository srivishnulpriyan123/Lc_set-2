int missingNumber(int* n, int ns) {
    int i,j,c,a;
    for(i=0;i<=ns;i++)
    {
        j=0,c=0;
        while(j<=ns-1)
        {
            if(n[j]==i)
            {
                c=1;
                break;
            }
            j++;
        }
       if(c==0)
       {
        a=i;
        break;
       } 
    }
    return a;
}
